{% extends 'layout/base.html.twig' %}

{% block content %}
    <h1 class="font-bold">Zebrina</h1>

    {% if devices %}
    <ul>
        {% for device in devices %}
            <li><a href="{{ path('device', {device: device}) }}">{{ device }}</a></li>
        {% endfor %}
    </ul>
    {% endif %}
{% endblock %}
